<%# <p><strong>Your Name:</strong> </p> %>
<%= form_tag signedin_path, method: :post do %>
    <%= label_tag :name, "Your name: "%>
    <%= collection_select :user, :name, User.all, :name, :name %><br><br>
    <%= label_tag :password, "Your password: " %>
    <%= password_field_tag :password %><br><br>
    <%= submit_tag "Sign In" %>
<% end %>